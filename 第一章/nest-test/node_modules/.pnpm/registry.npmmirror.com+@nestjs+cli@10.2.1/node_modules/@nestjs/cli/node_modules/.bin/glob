#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/esm/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules/glob/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/esm/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules/glob/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/registry.npmmirror.com+glob@10.3.10/node_modules:/Users/wuzaifa/Gauche/CoderGauche/nest/learn/第一章/nest-test/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
